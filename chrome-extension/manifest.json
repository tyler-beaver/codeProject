{
  "manifest_version": 3,
  "name": "Dashboard Auto-Fill",
  "version": "1.0",
  "description": "Auto-fills the dashboard field on submit instead of the email field.",
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
